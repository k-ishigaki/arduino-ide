-I/root/.platformio/packages/framework-arduino-avr/variants/standard
-I/root/.platformio/packages/framework-arduino-avr/libraries/EEPROM/src
-I/root/.platformio/packages/framework-arduino-avr/libraries/HID/src
-I/root/.platformio/packages/framework-arduino-avr/libraries/SPI/src
-I/root/.platformio/packages/framework-arduino-avr/libraries/SoftwareSerial/src
-I/root/.platformio/packages/framework-arduino-avr/libraries/Wire/src
-I/root/.platformio/packages/toolchain-atmelavr/avr/include
-I/root/.platformio/packages/toolchain-atmelavr/lib/gcc/avr/5.4.0/include
-I/root/.platformio/packages/toolchain-atmelavr/lib/gcc/avr/5.4.0/include-fixed
-I/root/.platformio/packages/tool-unity
-I/root/project_root/src
# defines
-DPLATFORMIO=30606
-DARDUINO_AVR_UNO
-DF_CPU=16000000L
-DARDUINO_ARCH_AVR
